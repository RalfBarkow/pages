        "text": "sub traverse {\n    local($title) = pop(@_);\n    $DonePages{$title}++;\n    local (%old) = &retrieve($title);\n    local ($text) = \"\";\n    local ($lines) = $old{text};\n    local ($_);\n    $lines =~ s/\\\\\\n//g;\n    foreach (split(/\\n/, $lines)){\n        next if /^(\\t+)(.+):\\t/;  \n        next if /^(\\t+)\\*/;\n        next if /^(\\t+)\\d+\\.?/;\n        next if /^\\s*$/;\n        next unless s/^\\s+//;\n        s/\\[Blank\\s*([^\\]]*)\\]/&blank($1)/eo;\n        $text .= \"$_\\n\";\n        while ($_ =~ s/($link)//) {\n            &traverse($1) unless $DonePages{$1};\n        }\n\n    }\n    print \"$text# -------------------  $title\\n\"\n      if $text;\n}\n"
