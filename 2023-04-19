    },
    {
      "type": "code",
      "id": "d1d53bf044a24b38",
      "text": "module Main exposing (..)\n\nimport Html exposing (..)\nimport Html.Events exposing (onClick, onInput)\nimport Html.Attributes exposing (src, placeholder)\n\n\ntype Msg\n    = ImageClicked\n    | InputChanged String\n\n\nmain : Html Msg\nmain =\n    div []\n        [ img [ src \"https://via.placeholder.com/350x150\", onClick ImageClicked ] []\n        , input [ placeholder \"Type your message here...\", onInput InputChanged ] []\n        ]\n\n\nupdate : Msg -> model -> ( model, Cmd Msg )\nupdate msg model =\n    case msg of\n        ImageClicked ->\n            ( model, Cmd.none )\n\n        InputChanged value ->\n            ( model, Cmd.none )\n"
    },
